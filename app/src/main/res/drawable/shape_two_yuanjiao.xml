<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- rectangle表示为矩形 -->

    <!-- 填充的颜色 -->
    <solid android:color="@color/D3" />

    <!-- 边框的颜色和粗细 -->
    <stroke
        android:width="@dimen/divider_size_0.5"
        android:color="@color/D3"
        />

    <!-- android:radius 圆角的半径 -->
    <corners
        android:topLeftRadius="@dimen/view_space_20"
        android:topRightRadius="@dimen/view_space_20"
        android:bottomRightRadius="0dp"
        android:bottomLeftRadius="0dp"
        />

</shape>