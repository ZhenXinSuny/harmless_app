<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="@dimen/view_space_12"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!-- 检疫证号提示文本 -->
    <TextView
        android:id="@+id/cert_no_tips_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="检疫证号："
        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:paddingBottom="4dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <!-- 检疫证号值 -->
    <TextView
        android:id="@+id/cert_no_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="510002358"
        android:textSize="16sp"
        android:textColor="@color/J5"
        android:textStyle="bold"
        android:paddingBottom="4dp"
        app:layout_constraintTop_toTopOf="@id/cert_no_tips_tv"
        app:layout_constraintStart_toEndOf="@id/cert_no_tips_tv"
        app:layout_constraintBaseline_toBaselineOf="@id/cert_no_tips_tv"/>


    <TextView
        android:id="@+id/quantity_conform_tips_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="数量是否符合："
        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:paddingBottom="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cert_no_tips_tv"/>

    <!-- 检疫证号值 -->
    <TextView
        android:id="@+id/quantity_conform_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="是"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:paddingBottom="4dp"
        app:layout_constraintTop_toTopOf="@id/quantity_conform_tips_tv"
        app:layout_constraintStart_toEndOf="@id/quantity_conform_tips_tv"
        app:layout_constraintBaseline_toBaselineOf="@id/quantity_conform_tips_tv"/>






    <TextView
        android:id="@+id/actual_quantity_tips_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="实际数量："
        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:paddingBottom="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/quantity_conform_tips_tv"/>

    <!-- 检疫证号值 -->
    <TextView
        android:id="@+id/actual_quantity_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="是"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:paddingBottom="4dp"
        app:layout_constraintTop_toTopOf="@id/actual_quantity_tips_tv"
        app:layout_constraintStart_toEndOf="@id/actual_quantity_tips_tv"
        app:layout_constraintBaseline_toBaselineOf="@id/actual_quantity_tips_tv"/>


    <TextView
        android:id="@+id/abnormal_quantity_tips_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="异常数量："
        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:paddingBottom="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/actual_quantity_tips_tv"/>

    <!-- 检疫证号值 -->
    <TextView
        android:id="@+id/abnormal_quantity_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="是"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:paddingBottom="4dp"
        app:layout_constraintTop_toTopOf="@id/abnormal_quantity_tips_tv"
        app:layout_constraintStart_toEndOf="@id/abnormal_quantity_tips_tv"
        app:layout_constraintBaseline_toBaselineOf="@id/abnormal_quantity_tips_tv"/>


    <TextView
        android:id="@+id/deaths_num_tips_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="死亡数量："
        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:paddingBottom="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/abnormal_quantity_tips_tv"/>

    <!-- 检疫证号值 -->
    <TextView
        android:id="@+id/deaths_num_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="是"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:paddingBottom="4dp"
        app:layout_constraintTop_toTopOf="@id/deaths_num_tips_tv"
        app:layout_constraintStart_toEndOf="@id/deaths_num_tips_tv"
        app:layout_constraintBaseline_toBaselineOf="@id/deaths_num_tips_tv"/>


    <TextView
        android:id="@+id/change_times_tips_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="修改时间："
        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:paddingBottom="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/deaths_num_tips_tv"/>

    <!-- 检疫证号值 -->
    <TextView
        android:id="@+id/change_times_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="是"
        android:textSize="16sp"
        android:textColor="@color/J5"
        android:textStyle="bold"
        android:paddingBottom="4dp"
        app:layout_constraintTop_toTopOf="@id/change_times_tips_tv"
        app:layout_constraintStart_toEndOf="@id/change_times_tips_tv"
        app:layout_constraintBaseline_toBaselineOf="@id/change_times_tips_tv"/>



    <TextView
        android:id="@+id/change_people_tips_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="修改人："
        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:paddingBottom="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/change_times_tips_tv"/>

    <!-- 检疫证号值 -->
    <TextView
        android:id="@+id/change_people_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="是"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:paddingBottom="4dp"
        app:layout_constraintTop_toTopOf="@id/change_people_tips_tv"
        app:layout_constraintStart_toEndOf="@id/change_people_tips_tv"
        app:layout_constraintBaseline_toBaselineOf="@id/change_people_tips_tv"/>


    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/divider_size_0.5"
        android:layout_marginTop="@dimen/view_space_4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/change_people_tips_tv"
        android:background="@color/D1" />
</androidx.constraintlayout.widget.ConstraintLayout>
