                        android:layout_height="match_parent"
                        android:layout_marginRight="@dimen/view_space_8"
                        android:layout_weight="1"
                        android:gravity="right|center">

                        <RadioGroup
                            android:id="@+id/njrg6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:orientation="horizontal">

                            <RadioButton
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginRight="@dimen/view_space_12"
                                android:button="@drawable/selector_checkbox"
                                android:id="@+id/danganziliaoTwoOk"
                                android:onClick="onRadioButtonClickedNJOne"
                                android:padding="6dp"
                                android:text="是"
                                android:textColor="@color/T6"
                                android:textSize="@dimen/text_size_14" />

                            <RadioButton
                                android:id="@+id/danganziliaoTwoNo"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:button="@drawable/selector_checkbox"
                                android:onClick="onRadioButtonClickedNJOne"
                                android:padding="6dp"
                                android:text="否"
                                android:textColor="@color/T6"
