                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:orientation="horizontal">

                            <RadioButton
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginRight="@dimen/view_space_12"
                                android:button="@drawable/selector_checkbox"
                                android:id="@+id/danganziliaoOk"
                                android:onClick="onRadioButtonClickedNJOne"
                                android:padding="6dp"
                                android:text="是"
                                android:textColor="@color/T6"
                                android:textSize="@dimen/text_size_14" />

                            <RadioButton
                                android:id="@+id/danganziliaoOneNo"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:button="@drawable/selector_checkbox"
                                android:onClick="onRadioButtonClickedNJOne"
                                android:padding="6dp"
                                android:text="否"
                                android:textColor="@color/T6"
                                android:textSize="@dimen/text_size_14" />

                        </RadioGroup>
                    </LinearLayout>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/divider_size_0.5"
                    android:layout_marginLeft="@dimen/view_space_10"
                    android:layout_marginRight="@dimen/view_space_10"
                    android:background="@color/D6" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/view_space_68"
                    android:layout_marginLeft="@dimen/view_space_10"
                    android:layout_marginRight="@dimen/view_space_10"
                    android:background="@color/white"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:ellipsize="end"
                        android:maxEms="11"
                        android:maxLines="2"
                        android:text="建立了完整、准确的使用、培训等记录"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_16"></TextView>

