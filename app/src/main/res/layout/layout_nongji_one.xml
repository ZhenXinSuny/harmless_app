                        android:textSize="@dimen/text_size_16"></TextView>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginRight="@dimen/view_space_8"
                        android:layout_weight="1"
                        android:gravity="right|center">

                        <RadioGroup
                            android:id="@+id/njrg1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:orientation="horizontal">

                            <RadioButton
                                android:id="@+id/zhiziOk"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginRight="@dimen/view_space_12"
                                android:button="@drawable/selector_checkbox"
                                android:onClick="onRadioButtonClickedNJOne"
                                android:padding="6dp"
                                android:text="是"
                                android:textColor="@color/T6"
                                android:textSize="@dimen/text_size_14" />

                            <RadioButton
                                android:id="@+id/zhiziNo"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:button="@drawable/selector_checkbox"
                                android:onClick="onRadioButtonClickedNJOne"
                                android:padding="6dp"
                                android:text="否"
                                android:textColor="@color/T6"
                                android:textSize="@dimen/text_size_14" />

                        </RadioGroup>
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/F8">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/view_space_32"
                        android:layout_marginLeft="@dimen/view_space_10"
                        android:gravity="center"
